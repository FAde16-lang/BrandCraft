# BizForge Environment Configuration
# Copy this file to .env and fill in your values

# Groq Cloud API Key (required)
# Get your key from: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Optional: Hugging Face Token for better image generation (SDXL)
# Get one for free at https://huggingface.co/settings/tokens
HF_API_TOKEN=

# Google Authentication
GOOGLE_CLIENT_ID=your_groq_api_key_here

# Model Configuration
MODEL_NAME=llama-3.3-70b-versatile

# Server Configuration (optional)
HOST=0.0.0.0
PORT=8000
DEBUG=true
